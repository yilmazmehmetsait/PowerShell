Get-Mailbox -ResultSize Unlimited -Filter {RecipientTypeDetails -eq "UserMailbox"} | Set-Mailbox -AuditEnabled $true -AuditDelegate Update,MoveToDeletedItems,SoftDelete,HardDelete,SendAs,SendOnBehalf,Create,FolderBind,UpdateFolderPermissions,UpdateInboxRules,Move -AuditOwner MailboxLogin,SoftDelete,HardDelete,UpdateFolderPermissions,UpdateInboxRules,UpdateCalendarDelegation,Create,Move,MoveToDeletedItems,Update -AuditAdmin Update,MoveToDeletedItems,SoftDelete,HardDelete,MessageBind,SendAs,SendOnBehalf,Create,Copy,FolderBind,UpdateFolderPermissions,UpdateInboxRules,UpdateCalendarDelegation,Move
